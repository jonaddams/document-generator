# Production Environment Variables

# Nutrient Document Authoring SDK
NEXT_PUBLIC_NUTRIENT_LICENSE_KEY=your_production_license_key

# Application Settings
NEXT_PUBLIC_APP_NAME=Document Generator
NEXT_PUBLIC_APP_DESCRIPTION=Create professional documents with Nutrient SDK

# Production Settings
NODE_ENV=production
NEXT_PUBLIC_DEBUG=false

# Production API Configuration
# API_BASE_URL=https://your-api-domain.com
# API_TIMEOUT=30000

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_ERROR_REPORTING=true

# Security Settings (use actual values in production)
# SESSION_SECRET=your_secure_session_secret
# ENCRYPTION_KEY=your_secure_encryption_key

# Performance Monitoring
# NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn
# VERCEL_ANALYTICS_ID=your_vercel_analytics_id